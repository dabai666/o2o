            <button type="button" id="map-refresh-pos-2" class="btn "><i class="fa fa-refresh"></i> 重置范围</button>
            <div class="blank5"></div>
            <div id="map-container-2" style="width:540px; height:300px; border:1px solid #ccc;"></div>
            <input type="hidden" name="mapPoint" id="map-point-2" value="<?php echo e($data['mapPoint']); ?>"/>
            <input type="hidden" name="mapPos" id="map-pos-2" value="<?php echo e($data['mapPos']); ?>"/>
            <script type="text/javascript">
            var qqGeocoder2,qqMap2,qqMarker2,qqPolygon2 = null,qqLatLngs2 = null;
            var defaultMapPoint2 = "<?php echo e($data['mapPoint']); ?>";
            jQuery(function($){
                $(window).load(function(){
                    var mapCenter2;
                    if(defaultMapPoint2 == ""){
                        mapCenter2 = new qq.maps.LatLng(39.916527,116.397128);
                    } else {
                        mapCenter2 = new qq.maps.LatLng(<?php echo e($data['mapPoint']); ?>);
                    }           
                    qqMap2 = new qq.maps.Map(document.getElementById("map-container-2"),{
                        center: mapCenter2,
                        zoom: 13
                    });
                    qqMarker2 = new qq.maps.Marker({
                        map:qqMap2,
                        draggable:true,
                        position: mapCenter2
                    });

                    $("#map-refresh-pos-2").click(function(){
                        var latLng = mapCenter2;
                        var tmpLng = qq.maps.geometry.spherical.computeOffset(latLng, 500, 0);
                        //西北角
                        var wnLatLng = qq.maps.geometry.spherical.computeOffset(tmpLng, 500, -90);
                        //东北角
                        var enLatLng = qq.maps.geometry.spherical.computeOffset(tmpLng, 500, 90);
                        //东南角
                        var esLatLng = qq.maps.geometry.spherical.computeOffset(enLatLng, 1000, 180);
                        //西南角
                        var nwLatLng = qq.maps.geometry.spherical.computeOffset(wnLatLng, 1000, 180);
                        qqLatLngs2 = [wnLatLng,enLatLng,esLatLng,nwLatLng];
                        qqPolygon2.setPath(qqLatLngs2);
                    });

                    $.createPolygon2 = function(latLng){
                        if(!qqPolygon2){
                            var tmpLng = qq.maps.geometry.spherical.computeOffset(latLng, 500, 0);
                            //西北角
                            var wnLatLng = qq.maps.geometry.spherical.computeOffset(tmpLng, 500, -90);
                            //东北角
                            var enLatLng = qq.maps.geometry.spherical.computeOffset(tmpLng, 500, 90);
                            //东南角
                            var esLatLng = qq.maps.geometry.spherical.computeOffset(enLatLng, 1000, 180);
                            //西南角
                            var nwLatLng = qq.maps.geometry.spherical.computeOffset(wnLatLng, 1000, 180);
                            qqLatLngs2 = [wnLatLng,enLatLng,esLatLng,nwLatLng];

                            qqPolygon2 = new qq.maps.Polygon({
                                map:qqMap2,
                                editable:true,
                                visible:true,
                                path:qqLatLngs2
                            });
                        } else {
                            var heading = qq.maps.geometry.spherical.computeHeading(mapCenter2, latLng);
                            var distance = qq.maps.geometry.spherical.computeDistanceBetween(mapCenter2, latLng);
                            qqLatLngs2 = new Array();
                            qqPolygon2.getPath().forEach(function(element, index){
                                qqLatLngs2.push(qq.maps.geometry.spherical.computeOffset(element, distance, heading));
                            });
                            qqPolygon2.setPath(qqLatLngs2);
                        }
                        mapCenter2 = latLng;
                    }

                    qq.maps.event.addListener(qqMarker2, "dragend", function(event) {
                        $("#map-point-2").val(event.latLng.getLat() + "," + event.latLng.getLng());
                        $.createPolygon2(event.latLng);
                    });
                    qq.maps.event.addListener(qqMap2, "click", function(event) {
                        qqMarker2.setPosition(event.latLng);
                        $.createPolygon2(event.latLng);
                        $("#map-point-2").val(event.latLng.getLat() + "," + event.latLng.getLng());
                    });

                    if(defaultMapPoint2 == ""){
                        var cityLocation2 = new qq.maps.CityService({
                            complete : function(result){
                                qqMap2.setCenter(result.detail.latLng);
                                qqMarker2.setPosition(result.detail.latLng);
                                $.createPolygon2(result.detail.latLng);
                                $("#map-point-2").val(result.detail.latLng.getLat() + "," + result.detail.latLng.getLng());
                            }
                        });
                        cityLocation2.searchLocalCity();
                    } else {
                        var mapPos2 = "<?php echo e($data['mapPos']); ?>".split("|");
                        var mpLatLng2;
                        qqLatLngs2 = new Array();
                        for(var mpIndex = 0; mpIndex < mapPos2.length; mpIndex++){
                            mpLatLng2 = mapPos2[mpIndex].split(",");
                            qqLatLngs2.push(new qq.maps.LatLng(mpLatLng2[0],mpLatLng2[1]));
                        }
                        qqPolygon2 = new qq.maps.Polygon({
                            map:qqMap2,
                            editable:true,
                            visible:true,
                            path:qqLatLngs2
                        });
                    }
                    
                    qqGeocoder2 = new qq.maps.Geocoder({
                        complete : function(result){
                            qqMap2.setCenter(result.detail.location);
                            qqMarker2.setPosition(result.detail.location);
                            $.createPolygon2(result.detail.location);
                            $("#map-point-2").val(result.detail.location.getLat() + "," + result.detail.location.getLng());
                        }
                    });

                    $("#map-search-2").click(function(){
                        if($.trim($("#map-address-2").val()) != ""){
                            qqGeocoder2.getLocation($("#map-address-2").val());
                        }
                    });

                    YZ.AJAX_FROM_SYNC.push(function(){
                        var maplatLngs = new Array();
                        qqPolygon2.getPath().forEach(function(element, index){
                            maplatLngs.push(element.getLat() + "," + element.getLng());
                        });
                        $("#map-pos-2").val(maplatLngs.join("|"));
                    });
                })
            })
            </script>
		            </div>
		        </div>
